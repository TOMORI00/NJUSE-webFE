<script>
    $(function() {
        $('.imgBox').lazyLoad({
            preOffset: 80,
            container: document.getElementById('container')
        })
        $('.imgBox').click(function() {
            var test = /_min\.webp/;
                if (test.test($(this).attr("src"))) {
                    var reSrc = $(this).attr("src").replace(test, "\.png");
                    $(this).attr("src", reSrc)
